  "cSpell.words": ["libretro", "retroarch", "romm"],
  "bun.runtime": "/var/home/tagho/.local/share/npm/bin/bun",
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  }
}
